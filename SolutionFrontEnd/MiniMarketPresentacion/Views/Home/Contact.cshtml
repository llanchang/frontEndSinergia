@model IEnumerable<MiniMarketPresentacion.Models.Producto>




<h2>Productos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.medidas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cantidad)
        </th>
        <th> 
            @Html.DisplayNameFor(model => model.precio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.marcaid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.proveedorid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.categoriaid)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.medidas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.marcaid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.proveedorid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.categoriaid)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>



